{
	"name": "tomaskarlik/select2input",
	"type": "library",
	"description": "Select2 form input for Nette",
	"keywords": ["input", "forms", "nette", "select2"],
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{ "name": "Tomáš Karlík", "homepage": "http://tomaskarlik.cz" }
	],
	"require": {
		"php": ">=7.4",
		"nette/application": "^3.0",
		"nette/di": "^3.0",
		"nette/forms": "^3.0",
		"nette/php-generator": "^3.0",
		"nette/utils": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"TomasKarlik\\Select2Input\\": "src"
		}
	},
	"require-dev": {
		"phpstan/phpstan": "^1.0"
	},
	"scripts": {
		"phpstan": "php ./vendor/phpstan/phpstan/phpstan analyse -l max src"
	}
}
